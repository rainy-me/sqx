{
  "name": "sqx",
  "version": "0.0.1",
  "license": "MIT",
  "bin": {
    "sqx": "./dist/cli.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "vite client",
    "dev:server": "node script/build.js --watch --entry server/index.ts --out dist/server.js",
    "dev:cli": "node script/build.js --run --entry cli/index.ts --out dist/cli.js -",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && yarn build:cli && vite build client",
    "build:cli": "node script/build.js --entry cli/index.ts --out dist/cli.js",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@sqltools/formatter": "^1.2.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.3.2",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "cac": "^6.7.3",
    "concurrently": "^6.2.0",
    "connection-string": "^4.3.4",
    "esbuild": "^0.12.15",
    "execa": "^5.1.1",
    "fastify": "^3.19.1",
    "fastify-static": "^4.2.2",
    "fastify-websocket": "^3.2.0",
    "knex": "^0.95.7",
    "lodash.debounce": "^4.0.8",
    "monaco-editor": "^0.26.0",
    "mysql": "^2.18.1",
    "pg": "^8.6.0",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "vue": "^3.0.5",
    "vue-tsc": "^0.0.24"
  },
  "publishConfig": {
    "access": "public"
  }
}
